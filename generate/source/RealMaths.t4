        public static Boolean IsZero(Single value)
        {
            Single ep;
            Epsilon(out ep);
            return Abs(value) < ep;
        }

		/// <summary>
		/// todo
		/// </summary>
        public static Boolean IsZero(Double value)
        {
            Double ep;
