        // Variant Maths //---------------------------------------------------//

        /// <summary>
        /// Variant function.
        /// </summary>
        public static <#= type #> Distance (
            Vector3 vector1, Vector3 vector2)
        {
            <#= type #> result;
            Distance (ref vector1, ref vector2, out result);
            return result;
        }

        /// <summary>
        /// Variant function.
        /// </summary>
        public static <#= type #> DistanceSquared (
            Vector3 vector1, Vector3 vector2)
        {
            <#= type #> result;
            DistanceSquared (ref vector1, ref vector2, out result);
            return result;
        }

